<div class="row">

<% for (var i=28;i<31;i++ )  { %>
<div class="col p-3 bg-ali border">
<%= i %>
</div>
<% } %><!-- for-->

<% for (var i=0;i<4;i++ )  { %>
<div class="col p-3 bg-ali border">
<%= (cal[i].date).toString().slice(7) %>
<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %><!-- for-->

</div><!-- row-->

<div class="row">

<% for (var i=4;i<9;i++ )  { %>
<div class="col p-3 bg-ali border">

<%= (cal[i].date).toString().slice(7) %>

<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

<% for (var i=9;i<11;i++ )  { %>
<div class="col p-3 bg-ali border">

<%= (cal[i].date).toString().slice(6) %>

<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>


</div><!-- row-->

<div class="row">
<% for (var i=11;i<18;i++ )  { %>
<div class="col p-3 bg-ali border">
    <%= (cal[i].date).toString().slice(6) %>
    <span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

</div><!-- row-->

<div class="row">
<% for (var i=18;i<25;i++ )  { %>
<div class="col p-3 bg-ali border">

<%= (cal[i].date).toString().slice(6) %>

<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>
</div><!-- row-->

<% if (cal.length>28) { %>
<div class="row">
<% for (var i=25;i<30;i++ )  { %>
<div class="col p-3 bg-ali border">

<%= (cal[i].date).toString().slice(6) %>

<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

<% for (var i=1;i<3;i++ )  { %>
<div class="col p-3 bg-ali border">
<%= i %>
</div>
<% } %><!-- for-->
</div><!-- row-->

</div><!-- row-->

<% } %>

