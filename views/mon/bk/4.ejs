<div class="row">

<div align="center" class="col p-3 border">sun</div>
<div align="center" class="col p-3 border">mon</div>
<div align="center" class="col p-3 border">t</div>
<div align="center" class="col p-3 border">w</div>
<div align="center" class="col p-3 border">t</div>
<div align="center" class="col p-3 border">f</div>
<div align="center" class="col p-3 border">sat</div>
</div><!-- row-->

<div class="row">

<div class="col p-3 bg-ali border">
    31
</div>

<% for (var i=0;i<6;i++ )  { %>
<div class="col p-3 bg-ali border">
<%= cal[i].date.toString().slice(7) %>
</div>
<% } %><!-- for-->
</div><!-- row-->

<div class="row">

<% for (var i=6;i<13;i++ )  { %>
<div class="col p-3 bg-ali border">

<%= (cal[i].date).toString().slice(7)%>

<span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

</div><!-- row-->

<div class="row">
<% for (var i=13;i<20;i++ )  { %>
<div class="col p-3 bg-ali border">
    <%= (cal[i].date).toString().slice(6) %>
    <span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

</div><!-- row-->

<div class="row">
<% for (var i=20;i<27;i++ )  { %>
<div class="col p-3 bg-ali border">

    <%= (cal[i].date).toString().slice(6) %>

    <span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>
</div><!-- row-->

<% if (cal.length>28) { %>
<div class="row">
<% for (var i=27;i<30;i++ )  { %>
<div class="col p-3 bg-ali border">

    <%= (cal[i].date).toString().slice(6) %>

    <span class="bg-mis"><a href="<%= cal[i].des %>" target="_blank" ><%= cal[i].eve %> </a></span>
</div>
<% } %>

<% for (var i=1;i<5;i++ )  { %>
<div class="col p-3 bg-ali border">
<%= i %>
</div>
<% } %><!-- for-->
</div><!-- row-->

</div><!-- row-->

<% } %>



